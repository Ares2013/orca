{
  "name": "image-exporter",
  "version": "0.0.1",
  "description": "Plotly's image exporting utility",
  "license": "MIT",
  "main": "src/index.js",
  "bin": {
    "plotly-graph-exporter": "./bin/plotly-graph-exporter.js",
    "plotly-export-server": "./bin/plotly-export-server.js"
  },
  "scripts": {
    "start:server": "./bin/plotly-export-server.js --debug --plotly=../plotly.js/build/plotly.js",
    "start:runner": "./bin/plotly-graph-exporter.js --debug --plotly=../plotly.js/build/plotly.js",
    "test:lint": "standard | snazzy",
    "test:integration": "tap test/integration/*_test.js",
    "test:unit": "tap test/unit/*_test.js",
    "test": "npm run test:lint && npm run test:unit && npm run test:integration",
    "cover": "npm run test:unit -- --cov",
    "lint": "standard --fix"
  },
  "author": "Plotly, Inc.",
  "keywords": [
    "graphing",
    "plotting",
    "visualization",
    "plotly"
  ],
  "dependencies": {
    "body": "^5.1.0",
    "electron": "^1.6.11",
    "fast-isnumeric": "^1.1.1",
    "get-stdin": "^5.0.1",
    "glob": "^7.1.2",
    "is-plain-obj": "^1.1.0",
    "is-url": "^1.2.2",
    "minimist": "^1.2.0",
    "request": "^2.81.0",
    "run-parallel-limit": "^1.0.3",
    "semver-regex": "^1.0.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "snazzy": "^7.0.0",
    "spectron": "^3.7.2",
    "standard": "^10.0.2",
    "tap": "^10.7.1"
  },
  "nyc": {
    "exclude": [
      "build",
      "test"
    ]
  }
}
